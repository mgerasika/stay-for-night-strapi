{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Request",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "city": {
      "type": "string"
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "biginteger"
    },
    "identity": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "email": {
      "type": "email"
    },
    "description": {
      "type": "richtext"
    },
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "inversedBy": "request"
    }
  }
}
